# Pin assignment for LEDs
NET "C" LOC = "M11" ; # Bank = 2, Signal name = LD1
NET "S" LOC = "M5" ;  # Bank = 2, Signal name = LD0

# Pin assignment for SWs
NET "In2" LOC = "K3";  # Bank = 3, Signal name = SW2
NET "In1" LOC = "L3";  # Bank = 3, Signal name = SW1
NET "In0" LOC = "P11";  # Bank = 2, Signal name = SW0